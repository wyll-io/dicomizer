{
  "version": "0.0.0",
  "scripts": {
    "gen-css": "tailwindcss --input input.css --output public/css/tailwind.css"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "prettier": "^3.1.1",
    "prettier-plugin-go-template": "^0.0.15",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.3.6"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss",
      "prettier-plugin-go-template"
    ],
    "overrides": [
      {
        "files": [
          "*.html"
        ],
        "options": {
          "parser": "go-template"
        }
      }
    ],
    "goTemplateBracketSpacing": true
  }
}
